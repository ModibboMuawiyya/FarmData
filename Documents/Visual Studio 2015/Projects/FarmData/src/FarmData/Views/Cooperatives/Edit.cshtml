@model FarmData.Models.Cooperatives

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<form asp-action="Edit">
    <div class="form-horizontal">
        <h4>Cooperatives</h4>
        <hr />
        <div asp-validation-summary="ValidationSummary.ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="CooperativeId" />
        <div class="form-group">
            <label asp-for="ClusterId" class="control-label col-md-2">ClusterId</label>
            <div class="col-md-10">
                <select asp-for="ClusterId" class="form-control" />
                <span asp-validation-for="ClusterId" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="ClusterNumber" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ClusterNumber" class="form-control" />
                <span asp-validation-for="ClusterNumber" class="text-danger" />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

<div>

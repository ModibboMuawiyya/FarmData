@model IEnumerable<Farming.Models.BioData>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EntryDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FarmerPic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FingerPrint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Signature)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EntryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FarmerPic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FingerPrint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Signature)
        </td>
        <td>
            <a asp-action="Edit" asp-route-id="@item.FarmerId">Edit</a> |
            <a asp-action="Details" asp-route-id="@item.FarmerId">Details</a> |
            <a asp-action="Delete" asp-route-id="@item.FarmerId">Delete</a>
        </td>
    </tr>
}
</table>
